{
  "name": "eduontology-engine",
  "version": "1.0.0",
  "description": "EduOntology Core Engine - AI-Powered Learning Platform",
  "main": "dist/index.js",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "npm run dev:engine",
    "dev:engine": "tsx watch src/index.ts",
    "dev:next": "next dev",
    "dev:all": "concurrently -n next,engine \"npm run dev:next\" \"npm run dev:engine\"",
    "build": "tsc",
    "build:next": "next build",
    "start": "node dist/index.js",
    "start:next": "next start",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "neo4j:setup": "tsx scripts/setup-neo4j.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@neo4j/graphql": "^5.0.0",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.0.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "neo4j-driver": "^5.0.0",
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.13",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.0.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.2.5",
    "jest": "^29.7.0",
    "postcss": "^8.4.0",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": "18.x"
  }
}
